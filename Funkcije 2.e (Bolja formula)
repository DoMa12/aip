#include <stdio.h>
#include <stdlib.h>
#include <math.h>
float fun( float a, float b,float c){
  	
	return (-b+sqrt(b*b-4*a*c))/(2*a);
}  
float fun2(float a, float b, float c){
  	
	return (-b-sqrt(b*b-4*a*c))/(2*a);
}  
 int main(){
 	float a;
    float b; 	
 	float c;
 	scanf("%f",&a);
 	scanf("%f",&b);
 	scanf("%f",&c);
 	printf("X1: %.2f\n",fun(a,b,c));
 	printf("X2: %.2f\n",fun2(a,b,c));
 	
 	return 0;
 	
 }
 

	
